{
  "private": true,
  "scripts": {
    "build": "npm explore nodejs-extensions -- pnpm build",
    "build:styles": "npm explore nodejs-extensions -- pnpm build:styles",
    "compile": "npm explore nodejs-extensions -- pnpm compile",
    "lint": "npm explore nodejs-extensions -- pnpm lint",
    "clean": "npm explore nodejs-extensions -- pnpm clean",
    "watch": "npm explore nodejs-extensions -- pnpm watch"
  },
  "nodejsExtensions": {
    "assetsToCopy": [
      {
        "sources": [
          "Assets/Images"
        ],
        "target": "wwwroot/images"
      },
      {
        "sources": [
          "Assets/Fonts"
        ],
        "target": "wwwroot/fonts"
      }
    ],
    "styles": {
      "source": "Assets/Styles",
      "target": "wwwroot/css"
    }
  },
  "dependencies": {
    "bootstrap": "5.3.0"
  },
  "devDependencies": {
    "eslint": "8.47.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-promise": "6.1.1"
  }
}
